{
    "manifest_version": 2,
    "name": "SQL Tool Extension",
    "description": "Enhance SQL Tool",
    "version": "0.3.0",
    "web_accessible_resources": ["libs/*", "css/*", "js/*"],
    "content_scripts": [
        {
            "matches": ["*://*/*/querySqlTool/goToIndex.action"],
            "js": ["js/start.js"]
        }
    ],
    "page_action": {
      "default_popup": "popup.html"
    }
}